// Calculate fullness
var percent = global.currentSolar / global.maxSolar;
var diff = right - left;
var mid = left + diff * percent;

// Draw current solar bar
draw_rectangle_colour(
  left, top, mid, bottom,
  c_green, c_green, c_green, c_green,
  false
);

// Draw missing solar bar
draw_rectangle_colour(
  mid, top, right, bottom,
  c_red, c_red, c_red, c_red,
  false
);

// Draw outline
var thickness = 3;
for (var i = 0; i &lt; thickness; ++i) {
  draw_rectangle_colour(
    left + i, top + i, right - i, bottom - i,
    c_black, c_black, c_black, c_black,
    true
  );
}

</string>

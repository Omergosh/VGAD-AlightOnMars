  <code>// Light levels
global.min_light_level = 0;
global.max_light_level = 100;
global.default_light_level = 75;

// Cloud types
enum clouds {
  circle,
  ellipse,
  
  // Don't add after this, this is used as a fencepost to randomly select from
  // the previous types
  end_cloud 
};

// Cloud array accessors
enum cloudA {
  px = 0,
  py = 1,
  vx = 2,
  vy = 3,
  ct = 4,
  cd = 5,
  cp = 6,
};

// Circle cloud parameters accessors
enum circleA {
  radius = 0
};

// Ellipse cloud parameters accessors
enum ellipseA {
  xradius = 0,
  yradius = 1
};

// Game dimension info
var tiles_w = room_width / 32;
var tiles_h = room_height / 32;
global.tw = tiles_w;
global.th = tiles_h;

// list of (pixel_x, pixel_y, x_vel, y_vel, cloudType, cloudDarkness,
// cloudTypeParams)
global.clouds = ds_list_create();

// Start us with some clouds and tick them a few times to make the world seem
// prepopulated
var tickPerNew = 40;
var toCreate = 5;
repeat (toCreate) {
  spawnCloud();
  repeat(tickPerNew) {
    advanceClouds();
  }
}

// Delay the next spawn
global.cloudSpawnDelay = 100; // Amount to delay by
global.untilCloudSpawn = global.cloudSpawnDelay; // Counter

// Build tiles array
for (var j = 0; j &lt; tiles_h; ++j) {
    for (var i = 0; i &lt; tiles_w; ++i) {
        global.tiles[j, i] = instance_create(i * 32, j * 32, plainsObj);
    }
}</code>
